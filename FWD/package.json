{
  "name": "freshbites",
  "version": "1.0.0",
  "description": "A MERN stack application for food waste reduction in Bangalore",
  "main": "backend/server.js",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd backend && npm run dev",
    "client": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "install-all": "npm run install-server && npm run install-client",
    "install-server": "cd backend && npm install",
    "install-client": "cd frontend && npm install",
    "seed": "cd backend && node seedData.js",
    "start": "cd backend && npm start"
  },
  "dependencies": {
    "concurrently": "^7.6.0"
  },
  "devDependencies": {
  },
  "keywords": [
    "food",
    "waste-reduction",
    "mern",
    "deals",
    "bangalore",
    "sustainability",
    "react",
    "node",
    "express",
    "mongodb"
  ],
  "author": "FreshBites Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/freshbites/freshbites"
  },
  "bugs": {
    "url": "https://github.com/freshbites/freshbites/issues"
  },
  "homepage": "https://freshbites.com"
}
